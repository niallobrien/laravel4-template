{
    "require": {
        "laravel/framework": "4.0.*",
        "laravelbook/laravel4-powerpack": "dev-master"
        "raveren/kint": "dev-master"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },

    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev"
}